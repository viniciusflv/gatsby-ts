{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "npm run pre && cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "prod": "npm run build && npm run serve",
    "build": "npm run pre && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "pre": "npm run clean && npm run svg",
    "svg": "node $(pwd)/scripts/svgson.js $(pwd)/src/assets/svg",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,svg}\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "2.19.21",
    "gatsby-plugin-canonical-urls": "^2.2.1",
    "gatsby-plugin-google-analytics": "^2.2.0",
    "gatsby-plugin-intl": "^0.3.3",
    "gatsby-plugin-manifest": "^2.3.1",
    "gatsby-plugin-minify-html": "^1.0.1",
    "gatsby-plugin-nprogress": "^2.2.0",
    "gatsby-plugin-offline": "^3.1.0",
    "gatsby-plugin-page-creator": "^2.2.1",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.2.0",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-sitemap": "^2.3.1",
    "gatsby-plugin-styled-components": "^3.2.1",
    "gatsby-plugin-typescript": "^2.3.0",
    "gatsby-remark-code-buttons": "^2.0.4",
    "gatsby-remark-images": "^3.2.1",
    "gatsby-remark-lazy-load": "^1.0.2",
    "gatsby-remark-prismjs": "^3.4.1",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "^5.25.1",
    "html-minifier": "^4.0.0",
    "lazysizes": "^5.2.0",
    "prismjs": "^1.19.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "6.0.0-beta",
    "styled-components": "5.0.1",
    "svgo": "^1.3.2",
    "svgson": "^4.0.0",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@types/react": "^16.9.23",
    "@types/react-helmet": "^5.0.15",
    "@types/styled-components": "5.0.1",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "prettier": "^1.19.1",
    "tslint": "^6.1.1",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "4.2.0",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
